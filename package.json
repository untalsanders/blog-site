        "@types/node": "22.14.0",
        "eslint": "9.23.0",
        "globals": "16.0.0",
        "prettier": "3.5.3",
        "svelte": "5.20.1",
        "typescript": "5.7.3",
        "typescript-eslint": "8.29.0",
        "vite": "6.3.5"
